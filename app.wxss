.box-shadow {
    overflow: hidden;
    position: relative;
}

.box-shadow::before {
    position: absolute;
    content: "";
    display: block;
    left: 0;
    right: 0;
    bottom: 100%;
    height: 100rpx;
    box-shadow: 1px 0px 16px rgba(24,24,24,0.12);
}

.box-shadow1 {
    position: relative;
}

.box-shadow1::before {
    position: absolute;
    content: "";
    display: block;
    left: 0;
    right: 0;
    bottom: 90%;
    height: 10rpx;
    box-shadow: 0rpx -6rpx 10rpx 0rpx rgba(1,1,1,0.04);
    z-index: 999999;
}

.box {
    box-shadow: 0 0 18rpx 4rpx RGBA(0,0,0,0.14);
}

.flex {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
}

