.j-around {
    justify-content: space-around;
}

.icon-star {
    margin: 0 1rpx;
}

.animate {
    animation: suo 0.5s linear;
}

@-webkit-keyframes suo {
    0% {
        transform: scale(1.5);
    }

    20% {
        transform: scale(1.4);
    }

    40% {
        transform: scale(1.3);
    }

    60% {
        transform: scale(1.2);
    }

    80% {
        transform: scale(1.1);
    }

    100% {
        transform: scale(1);
    }
}

@keyframes suo {
    0% {
        transform: scale(1.5);
    }

    20% {
        transform: scale(1.4);
    }

    40% {
        transform: scale(1.3);
    }

    60% {
        transform: scale(1.2);
    }

    80% {
        transform: scale(1.1);
    }

    100% {
        transform: scale(1);
    }
}

