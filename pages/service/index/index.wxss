.quan {
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.6);
    position: fixed;
    left: 0;
    top: 0;
    z-index: 99999;
    justify-content: center;
}

.quan-con {
    width: 548rpx;
    background: transparent;
    transform: translateY(-40rpx);
}

.quan-head {
