@-webkit-keyframes towhite {
    0% {
        background: transparent;
    }

    20% {
        background: rgba(255,255,255,0.2);
    }

    40% {
        background: rgba(255,255,255,0.4);
    }

    60% {
        background: rgba(255,255,255,0.6);
    }

    80% {
        background: rgba(255,255,255,0.8);
    }

    100% {
        background: rgba(255,255,255,1);
    }
}

@keyframes towhite {
    0% {
        background: transparent;
    }

    20% {
        background: rgba(255,255,255,0.2);
    }

    40% {
        background: rgba(255,255,255,0.4);
    }

    60% {
        background: rgba(255,255,255,0.6);
    }

    80% {
        background: rgba(255,255,255,0.8);
    }

    100% {
        background: rgba(255,255,255,1);
    }
}

