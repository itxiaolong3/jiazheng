/*radio默认样式重置*/
radio {
      box-sizing: border-box;
      background: none;
      position: absolute;
      width: 100%;
      height: 100%;
      top:0;
      left:0;
      
      
}

radio .wx-radio-input {
      margin-right: 15rpx;
      position: absolute;
      width: 100%;
      height: 100%;
      top:0;
      left:5rpx;
      background: none;
      border-radius: 12rpx;
      box-sizing: border-box;
}

radio .wx-radio-input.wx-radio-input-checked {
      background-color:transparent;
      border-color:#FF2150;
      overflow: hidden;
      box-sizing: border-box;
      width: 160rpx;
      height: 70rpx;
      text-align: center;
      margin-left: 40rpx;
     
}
